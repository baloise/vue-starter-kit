{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "==================== INSTALLATION / SETUP ====================": "========",
    "prepare": "husky install",
    "postinstall": "baloiseStencilViteFix && npm run copy:fonts",
    "copy:fonts": "copyfiles --flat node_modules/@baloise/design-system-fonts/lib/* public/assets/fonts",
    "precommit": "lint-staged --allow-empty",
    "==================== FORMATTER / LINTER ======================": "========",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "npm run lint:code && npm run lint:style && npm run lint:app",
    "lint:code": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src cypress",
    "lint:style": "stylelint src/**/*.{css,scss,sass,vue}",
    "lint:app": "vue-tsc --noEmit",
    "spell": "cspell \"src/**/*.{md,mdx,ts,vue,sass,scss}\" \"cypress/**/*.{ts,md}\" --no-progress --gitignore",
    "==================== START ===================================": "========",
    "start": "npm run dev",
    "docs": "npm run storybook",
    "dev": "vite --port 3000",
    "preview": "vite preview --port 3000",
    "storybook": "start-storybook -p 6006",
    "==================== BUILD ===================================": "========",
    "build": "vue-tsc --noEmit && vite build",
    "build:docs": "npm run build:storybook",
    "build:storybook": "build-storybook",
    "==================== TEST ====================================": "========",
    "test": "npm run test:run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cy:install": "cypress install",
    "cy:open": "npm run cy:open:app",
    "cy:open:app": "cypress open",
    "test:e2e": "concurrently --names \"üß™_TEST__,üñ•Ô∏è__SERVER\" -c \"green,magenta\" \"npm:cy:open\" \"vite --port 3000 --mode e2e\"",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress run'"
  },
  "dependencies": {
    "@baloise/design-system-components-vue": "^10.12.1-beta.3",
    "@baloise/web-app-clean-architecture": "^3.3.2-beta.14",
    "@baloise/web-app-form-vue": "^3.3.2-beta.24",
    "@baloise/web-app-pipes": "^3.4.0",
    "@baloise/web-app-utils": "^3.4.0",
    "@baloise/web-app-validators": "^3.4.0",
    "@vueuse/core": "^8.5.0",
    "immer": "^9.0.14",
    "pinia": "^2.0.13",
    "pinia-logger": "^1.3.2",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@baloise/design-system-testing": "^10.12.0",
    "@baloise/stencil-vite-fix": "^10.10.8",
    "@geometricpanda/storybook-addon-iframe": "^0.2.1",
    "@mdx-js/react": "^1.6.22",
    "@rushstack/eslint-patch": "^1.1.3",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue3": "^6.5.6",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitest/ui": "^0.10.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "cspell": "^5.20.0",
    "cypress": "^9.6.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-terminal-report": "^3.5.2",
    "eslint": "^8.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^3.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.1",
    "vite": "^2.9.9",
    "vitest": "^0.10.5",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.35.2"
  }
}
